<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane prefHeight="520.0" prefWidth="960.0" stylesheets="@css/mainGame.css" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainGameController">
    <center>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="30.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="genericButton" mnemonicParsing="false" text="Mode Generic"/>
                <Button fx:id="nimButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Mode Nim"/>
                <Button fx:id="gameButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Mode Game"/>
                <Button fx:id="aboutButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="About"/>
                <Button fx:id="quitButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#quit"
                        text="Quit"/>
            </children>
        </VBox>
    </center>
</BorderPane>
